@using ProiectIS.Controllers
@{
    ViewData["Title"] = "Quiz Time!";
}

<div class="d-grid gap-2 col-6 mx-auto">

    <form>
        <a class="btn btn-primary" role="button" asp-controller="Student" asp-action="ViewPersonalData">View Personal Data</a>
         <a class="btn btn-primary" role="button" asp-controller="Quiz" asp-action="Index">Training</a>
         <a class="btn btn-primary" role="button" asp-controller="Notifications" asp-action="Index">Challenge Student!</a>
    </form>


    <button class="btn btn-primary btn-lg" type="button">Join Group</button>
    <button class="btn btn-primary me-md-2 btn-lg" type="button">Leave Group</button>
    

</div>